{
  "listeners": {
    "*:8080": {
      "pass": "applications/webapp"
    }
  },

  "applications": {
    "webapp": {
      "type": "python 3",
      "stdout": "/var/log/unit-app.log",
      "stderr": "/var/log/unit-app.log",
      "path": "/home/webapp",
      "working_directory": "/home/webapp",
      "home": ".venv",
      "module": "app",
      "callable": "app"
    }
  },

  "access_log": {
    "path": "/var/log/access.log"
  }
}
